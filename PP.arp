START
$5
''-------------------------------------------------------------------------
$12 MAIN PROGRAM LEADING LINES ''INIZIO MAIN'
NOME: [FNAME]
PARAMETRI: LX=[PANEL_X] LY=[PANEL_Y] LZ=[PANEL_Z]
UTENSILI USATI: [NMT]
OFFSET: XO=[XOFFSET] YO=[YOFFSET]
INIZIO PROGRAMMA
$15 MAIN PROGRAM TRAILING LINES   ''INIZIO MAIN
$17 FILE TRAILING LINES           ''FINE MAIN
FINE PROGRAMMA
$-----------------------------RAPIDI----------------------------------------
$20 MOVIMENTI IN RAPIDO IN XY ''RAPIDO IN XY
$LET MON=MON+1 ''CONTATORE RAPIDI XY PER SETTARE G0 INIZIALE DA ORIGINE
$LET ROUTER=1  ''INIZIALIZZA ROUTING (INDIPENDENTE)
$LET ENGRAVE=1 ''INIZIALIZZA ENGRAVE (INDIPENDENTE)
$LET DIST=SQR((AX-CX)**2+(AY-CY)**2+(AZ-CZ)**2) ''CALCOLO DISTANZA 3D
$IF DIST>DIST_REF ''INIZIALIZZA NUOVA LAVORAZIONE SE DIST>DIST_REF (FIRSTRAPID E FIRSTWORK) --> NUOVA LAVORAZIONE
$LET FIRSTRAPID=1 ''INIZIALIZZO RAPIDI G1 (DIPENDENTE)
$LET FIRSTWORK=1 ''INIZIALIZZO MACHINING (DIPENDENTE)
$ENDIF
$IF PAT(1)>0 ''MULTIDRILL NON USO
$GOTO 99
$ELSEIF T=2  ''LAMA NON HA RAPIDO
$GOTO 99
$ELSEIF TT=4 ''DRILL NON HA RAPIDO
$GOTO 99
$ELSEIF LASTLAV=0 ''LAME E FORI SOLO G0 ANCHE SE DIST<DIST_REF
$GOTO 30
$ELSEIF DIST<DIST_REF ''VAI A G0 TO G1
$GOTO 10
$ELSEIF LRB=1 ''ULTIMO RAPIDO PRIMA DEL PATH (NIN SERVE)
N[N] X[AX] Y[-AY+PANEL_Y] Z[CZ] G0
$GOTO 99
$ELSEIF FRA=1 ''PRIMO RAPIDO DOPO IL PATH (NON SERVE)
N[N] X[AX] Y[-AY+PANEL_Y] Z[CZ] G0
$GOTO 99
$ENDIF
$GOTO 99
''--------- G0 TO G1 (MOVIMENTO A MANO) -----------------------------------
$LL 10
$IF FIRSTRAPID+FIRSTWORK=2 ''PRIMA LAVORAZIONE METTO HEADER
$GOTO 20
$ELSEIF FIRSTRAPID=1
N[N] X[CX] Y[-CY+PANEL_Y] Z[CZ] G1''MOVIMENTO IN Z PER ALLINEARMI AL PIANO (SE NULLO MATLAB FILTRA)
N[N] X[AX-DELTA] Y[-AY+PANEL_Y] Z[CZ] G1''AZ=CZ --> MOVIMENTO XY IN G1 (CON DELTA)
$LET FIRSTRAPID=0
$ELSE
N[N] X[AX-DELTA] Y[-AY+PANEL_Y] Z[CZ] G1''AZ=CZ --> MOVIMENTO XY IN G1 (CON DELTA)
$LET FIRSTRAPID=0
$ENDIF
$GOTO 99
''------------------ ESCLUSIONE PRIMO RAPIDO (DA ORIGINE) ------------------
$LL 20 
$IF MON=1 ''LA MACCHINA DA ORIGINE SEMPRE IN RAPIDO --> MI POSIZIONO SU COORDINATE SUCCESSIVE
N[N] X[AX-DELTA] Y[-AY+PANEL_Y] Z[CZ] G0 ''(MATLAB FILTRA MOVIMENTO NULLO)
N[N] X[AX-DELTA] Y[-AY+PANEL_Y] Z=[CZ] TP=1 PRF=[-AZ] F=[F] VF=[F] S=[S] L=PON CHECKSPEED
N[N] X[AX-DELTA] Y[-AY+PANEL_Y] Z[CZ] G1''(CON DELTA)
$LET FIRSTRAPID=0
$ELSE ''MI POSIZIONO SU COORDINATE CORRENTI
N[N] X[CX-DELTA] Y[-CY+PANEL_Y] Z=[CZ] TP=1 PRF=[-AZ] F=[F] VF=[F] S=[S] L=PON CHECKSPEED
N[N] X[AX-DELTA] Y[-AY+PANEL_Y] Z[CZ] G1''(CON DELTA)
$LET FIRSTRAPID=0
$ENDIF
$GOTO 99
''--------- (FORI E LAME) SOLO RAPIDO G0 E NO INLAV -------------------------
$LL 30 
$IF LRB=1 ''ULTIMO RAPIDO PRIMA DEL PATH
N[N] X[AX] Y[-AY+PANEL_Y] Z[CZ] G0
$GOTO 99
$ELSEIF FRA=1 ''PRIMO RAPIDO DOPO IL PATH
N[N] X[AX] Y[-AY+PANEL_Y] Z[CZ] G0
$GOTO 99
$ENDIF
$GOTO 99
