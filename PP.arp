$GOTO 99
$ELSEIF T=2  ''LAMA NON METTO RAPIDO
$GOTO 99
$ELSEIF TT=4 ''DRILL NON METTO RAPIDO
$GOTO 99
$ELSEIF LASTLAV=0 ''LAME E FORI SOLO G0 
$GOTO 20
$ELSEIF DIST<DIST_REF ''VAI A G0 TO G1
$GOTO 10
$ELSEIF LRB=1 ''ULTIMO RAPIDO PRIMA DEL PATH
N[N] X[CX] Y[-CY+PANEL_Y] Z[AZ] G0
$GOTO 99
$ELSEIF FRA=1 ''PRIMO RAPIDO DOPO IL PATH
N[N] X[CX] Y[-CY+PANEL_Y] Z[AZ] G0
$GOTO 99
$ENDIF
$GOTO 99
''------------ MOVIMENTO VERTICALE MANUALE (G0 TO G1) ------------------------
$LL 10
$IF FIRSTRAPID+FIRSTWORK=2 ''INIZIO LAVORAZIONE CON MOVIMENTO Z (MAI USATO)
N[N] X[CX-DELTA] Y[-CY+PANEL_Y] Z=[CZ] TP=1 PRF=[-AZ] F=[F] VF=[F] S=[S] L=PON CHECKSPEED
$LET FIRSTRAPID=0
$ENDIF
$IF (AZ-CZ)<0 ''RAPIDI VERSO IL BASSO NON LI SCRIVO
N[N] X[CX] Y[-CY+PANEL_Y] Z[AZ] G0''LAVORAZIONI SUCCESSIVE
$ELSEIF (AZ-CZ)>0 ''RAPIDI VERSO ALTO LI SCRIVO
N[N] X[CX] Y[-CY+PANEL_Y] Z[AZ] G1''LAVORAZIONI SUCCESSIVE
$ENDIF
$GOTO 99
''--------------- (FORI E LAME) SOLO RAPIDO G0 -------------------------------
$LL 20
$IF LRB=1 ''ULTIMO RAPIDO PRIMA DEL PATH
N[N] X[CX] Y[-CY+PANEL_Y] Z[AZ] G0
$GOTO 99
$ELSEIF FRA=1 ''PRIMO RAPIDO DOPO IL PATH
N[N] X[CX] Y[-CY+PANEL_Y] Z[AZ] G0
$GOTO 99
$ENDIF
$GOTO 99
''---------------------------------------------------------------------------
$LL99 USCITA
$----------------------------------------------------------------------------
$30 LINES BEFORE A RAPID MOVE IF Z LEVELS ARE NOT USED EG LASER OR GEOMETRY 
$----------------------------------------------------------------------------
$35 LINES AFTER A RAPID MOVE IF Z LEVELS ARE NOT USED EG LASER OR GEOMETRY  
$----------------------- MACHINING FEED MOVES -------------------------------
$40  MACHINING FEED LINES ''FILTRO MOVIMENTI PICCOLI E PASSO A SWITCH(1000)
$MODAL OFF X Y Z
$LET D_X=ABS(AX-CX)
$LET D_Y=ABS(AY-CY)
$LET D_Z=ABS(AZ-CZ)
$IF D_X>D_X_REF
$GOTO 1000
$ELSEIF D_Y>D_Y_REF
$GOTO 1000
$IF D_Z>D_Z_REF
$GOTO 1000
$ENDIF
$GOTO 99
